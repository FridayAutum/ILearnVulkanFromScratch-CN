
{
    "title" : "Vulkan 学习笔记",
    "author" : "Gavin KG",
	"description" : "Vulkan 学习笔记",
    "language" : "zh-hans",
    "plugins" : [
        "-sharing",
		"-lunr",
		"github-buttons",
		"expandable-chapters-small",
		"splitter",
		"sitemap-general",
        "page-toc",
        "katex",
		"search-plus",
		"tbfed-pagefooter",
		"theme-fexa"
    ],
	"pluginsConfig": {
        "github-buttons": {
            "buttons": [{
                "user": "GavinKG",
                "repo": "ILearnVulkanFromScratch-CN",
                "type": "star",
                "size": "small"
              }]
        },
		"sitemap-general": {
			"prefix": "https://gavinkg.github.io/ILearnVulkanFromScratch-CN/"
		},
		"page-toc": {
			"selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
			"position": "before-first",
			"showByDefault": true
        },
        "tbfed-pagefooter": {
            "copyright":"",
            "modify_label": "该文件最后修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索",
            "logo":"./logo.jfif",
            "favicon": "./favicon.ico"
        }
    }
}